# Ignore bundler config.
/.bundle

# Ignore local gemfiles
Gemfile.local
Gemfile.local.lock

# Ignore jupyter notebooks
.ipynb_checkpoints/
.ipython/
*.ipynb

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log

/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore ENV variable files
.env
.env.local
/client/.node-env

# Elastic Beanstalk Files
.elasticbeanstalk/*
!.elasticbeanstalk/*.cfg.yml
!.elasticbeanstalk/*.global.yml

# Ignore files built by 'npm run build'
/client/dist/*

