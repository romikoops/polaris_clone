<% environment.context_class.instance_eval { include ApplicationHelper } %>

@import url("https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.css");
@import "normalize";

.footer {
  height: 20px;
  width: 100%;
}

.refrow {
 display: -webkit-box;
 justify-content: space-between;
 .ref {
   width: 45%;
 }
}

.wrapper {
  position: relative;
  margin: 0;
  width: 100%;
}
.wrapper-quotations {
  padding-left: 80px;
  padding-right: 60px;
  padding-top: 20px;
  color: #4F4F4F !important;
  font-family: 'Roboto', sans-serif !important;
  .title {
    display: -webkit-box;
    justify-content: space-between;
    .title-logo {
      text-align: center;
      width: 40%;
    }
    .title-left {
      width: 30%;
    }
    h1 {
      font-size: 18px !important;
      font-weight: bold !important;
      margin: 0;
    }
    p {
      margin: 0;
      font-size: 12px !important;
    }
    hr {
      max-width: 107px;
      border-style: solid;
      border-width: 1px;
      margin: 7px 0;
    }
  }
}
.agent-info {
  text-align: right;
  width: 30%;
  overflow-x: wrap;
}
.text-center {
  text-align: center;
}
.text-right {
  text-align: right;
}
.hr-right{
  border-right: 1px solid #096CAF;
}
.hr-bottom{
  border-bottom: 1px solid #096CAF;
}
.hr-top{
  border-top: 1px solid #096CAF;
}
.hr-left{
  border-left: 1px solid #096CAF;
}
.no-padding{
  padding: 0%;
}
.watermark {
  color: #6699ff;
  font-size: 100pt;
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  position: absolute;
  z-index: -1;
  left:200px;
  top:550px;
  text-align: center;
  opacity: 0.1;
}
.quote {
  font-size: 45px;
}
.notes {
  margin: 12px;
  font-size: 8px;
  width: 80%;
}
.wrapper-vertical {
  transform: rotate(-90deg);
  -webkit-transform: rotate(-90deg);
  text-align: center;
  height: 25mm;
  width: 25mm;
}
.vertical {
  white-space: nowrap;
  margin: 0;
  font-size: 90%;
}
.text{
  padding-left: 2px;
}
br{
  padding-bottom: 5px;
}
.logo-quotations {
  height: 230px;
  margin: 0 auto !important;
}
.border-none {
  border: 0 !important;
}
.dates {
  align-items: center;
  display: -webkit-box;
  justify-content: space-between;
  border-bottom: 3px solid #f5f5f5;
  padding-top: 10px;
  width: 100%;
  padding-bottom: 10px;
  div {
    width: 25%;
  }
  h4 {
    font-size: 12px;
    font-weight: bold;
    margin: 8px 0;
  }
  p {
    margin: 10px 0;
    font-size: 14px;
  }
}
.overview {
  padding-top: 30px;
}
.overview-child {
  background: #F8F8F8 !important;
  -webkit-print-color-adjust: exact !important;
}
.validity {
  padding: 5px 10px;
  border-bottom: none;
  display: flex;
  justify-content: space-between;
  align-items: center;
  display: -webkit-box;
  p {
    width: 50%;
  }
}
.remarks {
  margin-top: 35px;
  padding: 10px 15px;
  border-bottom: none;
  page-break-inside: avoid;
}
.disclaimer {
  margin-top: 35px;
  padding: 10px 5px;
  border-bottom: none;
}
.remarks ul {
  list-style-type: none;
  margin-bottom: 2px;
}
.remarks li {
  font-size: 10px;
}
.note-header {
  font-weight: bold;
  font-size: 16px;
}
.note-body {
  padding-left: 20px
}
.info {
  padding: 20px 35px;
  background: white;
  border: 1.5px solid #E0E0E0;
  border-bottom: none;
  display: flex;
  justify-content: space-between;
  align-items: center;
  display: -webkit-box;
}
.info i {
  font-size: 27px;
}
.info p {
  font-size: 15px;
  margin: 0;
}
.info p span {
  font-size: 18px;
  font-weight: bold;
}
.delivery-info {
  padding: 20px 35px;
  background: white;
  border: 1.5px solid #E0E0E0;
  border-bottom: none;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.delivery-info p {
  font-size: 14px;
  margin: 0;
}
.mot-icon {
  padding-right: 24px
}
.subtotal-price-single {
  background: #F8F8F8 !important;
  -webkit-print-color-adjust: exact !important;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 5px;
  display: -webkit-box;
}
.subtotal-currency-single {
  background: white !important;
  -webkit-print-color-adjust: exact !important;
  display: flex;
  justify-content: space-between;
  flex-direction: row !important;
  align-items: center;
  display: -webkit-box;
  padding: 5px;
}
.subtotal-currency-single h3 {
  width: 50%;
  margin: 8px 0;
  font-size: 15px;
  text-align: right;
  font-weight: bold;
}
.subtotal-currency-single h4 {

  font-weight: bold;
  margin: 8px 0;
  font-size: 12px;
  width: 50%;
}
.subtotal-price-single h3 {

  margin: 8px 0;
  font-size: 12px;
  text-align: right;
  font-weight: normal;

}
.subtotal-price-single h4 {

  margin: 8px 0;
  font-size: 12px;
  font-weight: normal;
}

.total-price-single {
  background: white;
  border-top: none;
  border: 1.5px solid #E0E0E0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 5px;
  background: #e6e4e4;
  display: -webkit-box;
}
.total-price-single h4 {
  font-size: 16px;
  font-weight: bold;
  margin: 10px 0;
  width: 70%;
}
.total-price-single p {
  font-size: 12px;
  margin: 10px 0;
  text-align: end;
}
.total-price-single h3 {
  font-size: 20px;
  font-weight: bold;
  text-align: right;
  margin: 8px 0;
  width: 30%;
}
.total-row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #F8F8F8;
  border: 1.5px solid #BDBDBD;
  display: -webkit-box;
}
.total-row h4 {
  font-style: italic;
  font-weight: bold;
  font-size: 20px;
  margin: 10px 0;
  width: 50%;
}
.total-row p {
  font-weight: bold;
  padding-right: 30px;
  font-size: 28px;
  text-align: right;
  margin: 0;
  width: 50%;
}
.header-logo {
  width: 150px;
  height: auto;
}
.sub-prices {
  margin-top: 10px;
  background: white;
  border: 2px solid #BDBDBD;
}
.all-row {
  margin-bottom: 60px;
  position: relative;
  border: 2px solid #4F4F4F;
}
.shipment-info {
  flex-grow: 1;
  width: 40%;
}
.charge-icons {
  padding-bottom: 5px;
  display: flex;
  width: 100%;
  justify-content: space-between;
  i {
    font-size: 14px;
  }
}
.charge-icons .col-xs-2 i {
  font-size: 60px !important;
}
.cargo-info {
  min-width: 110px;
  width: 50%;
}
.flip {
  transform: scaleX(-1);
}
.oo {
  max-height: 120px;
  width: 100%;
  opacity: 0;
}
.title-shipment-grey {
  border-bottom: 3px solid #E0E0E0;
  color: #4F4F4F;
  height: 100px;

  p:first-child {
    margin-top: 0;
    font-size: 50px;
    margin-bottom: 0;
    span {
      font-weight: bold;
    }
  }
  p:last-child {
    margin-top: 0;
    font-size: 37px;
    margin-bottom: 0;
    text-align: right;
  }
}
.title-shipments h1 {
  font-size: 20px !important;
  font-weight: bold !important;
  margin: 0;
}
.tenant-shipments h1 {
  font-size: 17px !important;
  font-weight: bold !important;
  margin: 0;
}
.hr-shipment {
  max-width: 100% !important;
  border-style: solid;
  border-width: 1px;
  margin: 14px 0 !important;
}
.main-title-shipment {
  display: -webkit-box;
  justify-content: space-between;
  align-items: center;
  div {
    width: 50%;
  }
}
.border-right {
  border-right: 2px solid #f5f5f5;
}
.ref {
  font-size: 14px;
  span {
    font-weight: bold;
  }
}
.user-icon {
  font-size: 60px;
  color: #B7B7B7;
  padding-right: 20px;
}
.padding-top {
  padding-top: 10px;
  page-break-inside: avoid;
}
.padding-top-half {
  padding-top: 15px;
  page-break-inside: avoid;
}
.padding-top-double {
  padding-top: 30px;
  page-break-inside: avoid;
}
.margin-left-negative {
  margin-left: -130px;
}
.display-flex {
  display: -webkit-box;
}
.display-flex-center {
  display: -webkit-box;
  justify-content: space-between;
  align-items: center;
  &.border-box {
    align-items: unset;
  }
}
.contacts-row {
  display: -webkit-box;
  justify-content: space-between;
  align-items: center;
  h3 {
    font-weight: normal !important;
    font-size: 10px;
  }
  p, h1, h2, h3 {
    margin: 0 auto;
  }
  .contact-box {
    margin: 2.5%;
    width: 40%;
  }
  .basic-info {
    i {
      font-size: 12px;
      padding-right: 14px;
    }
    h1, h2, h3 {
      flex-grow: 1;
    }
    > div:first-child {
      margin-bottom: 8px;
    }
    h1 {
      font-weight: bold;
      font-size: 14px;
    }
    h2 {
      font-size: 11px
    }
  }
  .contact-info {
    font-weight: 400;
    i {
      font-size: 12px;
      padding-right: 14px;
    }
    h1, h2, h3 {
      flex-grow: 1;
    }
    div {
      margin-bottom: 8px;
    }
  }
  .arrow-circle {
    width: 10%;
    font-size: 45px;
    color: #EAEAEA;
  }
}
.carriage-row {
  padding: 25px 0;
  p {
    font-size: 12px !important;
    margin: 0;
  }
  h4 {
    font-size: 13px;
    font-weight: bold;
    margin: 0;
    margin-bottom: 8px;
  }
  i {
    font-size: 15px;
    text-align: right;
    padding-right: 12px;
  }
}
.border-box {
  border: 3px solid #f5f5f5;
  overflow-x: none;
}
.border-bottom {
  border-bottom: 2px solid #f5f5f5;
}
.tenant-notes-row {
  padding: 25px;
  p {
    font-size: 12px;
  }
}
.notes-row {
  margin-top: 15px;
  span {
    font-size: 11px;
    font-weight: bold;
    padding-right: 4px;
  }
   p {
     margin: 0;
     font-size: 12px;
     font-weight: normal;
   }
   .notes-upper {
     justify-content: space-around;
     align-items: center;
   }
   .notes-upper div:first-child {
     padding-left: 8px;
   }
   .notes-upper > div {
     padding: 8px 0;
   }
   .notes-lower > div {
     padding: 8px 0 8px 8px;
   }
   .hyphens {
      hyphens: auto;
   }
}
.margin-bottom {
  margin-bottom: 17px;
}
.services-box {
  padding-left: 5%;
  hyphens: auto;
  width: 50%;
  overflow-x: none;
  p {
    margin:0;
    font-size: 10px;
    span {
      font-weight: bold;
    }
  }
  i {
    padding-right: 10px;
    font-size: 16px !important;
    &.flip {
      padding-right: 0;
      padding-left: 14px;
    }
  }
  .flip {
    transform: scaleX(-1);
  }
  h3 {
    margin-bottom: 35px;
    padding-top: 4px;
    font-size: 14px;
    font-weight: bold;
  }
}
.services-box.wide {
  width: 70%;
}
.third {
  width: 33%;
}
.half {
  width: 50%;
}
.whole {
  width: 100%;
}
.flex-grow {
  flex-grow: 1;
}
.additional-services {
  p {
    margin: 0;
    font-size: 10px;
  }
  h3 {
    margin-bottom: 16px;
    padding-top: 4px;
    font-size: 14px;
    font-weight: bold;
  }
  i {
    padding-right: 10px;
    font-size: 16px !important;
  }
  width: 20%;
}
.total-section {
  width: 19%;
  text-align: center;
  background: #f5f5f5;
  padding-left: 40px;
  padding-right: 40px;
  hyphens: auto;
  padding-top: 30px;
  p {
    font-size: 12px;
  }
  h2 {
    font-size: 16px;
  }
}
.cargo-details-title {
  height: 40px;
  background: #e6e4e4;
  h1 {
    margin: 0;
    font-size: 14px;
    font-weight: bold;
    padding-top: 10px;
    padding-left: 30px;
  }
}
.title_row {
  background: #CECECE;
  padding: 5px;
  p {
    margin: 0;
  }
}

.cargo-row {
  p {
    margin: 0;
  }
  .cargo-section {
    padding: 8px 8px;
    position: relative;
    .operand {
      position: absolute;
      right: -15px;
      top: 10px;
    }
  }
  .cargo-section-half {
    width: 50%;
    padding: 8px 8px;
    position: relative;
    .cargo_value {
      width: 100%;
      text-align: center;
    }
    .operand {
      position: absolute;
      right: -15px;
      top: 10px;
    }
  }
  .cargo-section-last {
    padding-right: 35px;
  }
  .cargo-items {
    p {
      font-size: 14px !important;
    }
  }
  .cargo-index {
    padding-left: 30px;
    p {
      font-size: 25px;
    }
  }
  .cargo-value {
    font-size: 11px;
    font-weight: bold;
  }
  .cargo-subtitle {
    font-size: 8px;
    flex-wrap: no-wrap;
  }
  .divider {
    height: 100%;
    width: 1px;
    position: absolute;
    right: -5px;
    top: 0;
    bottom: 0;
    border-right: 2px solid #F8F8F8;
  }
}
.cargo-details-all {
  page-break-inside: avoid;
}
.no_page_break {
  page-break-inside: avoid;
}
.item-group {
  border: 1px solid #CECECE;
}
.chargeable_weight {
  font-size: 10px;
}
.cargo_title {
  padding: 15px 5px 5px 5px;
  background: #CECECE;
  font-size: 14px;
}
.rate_wrapper {
  display: flex;
  display: -webkit-box;
}
.rate_section {
  width: 20%;
  .rate_header {
    margin: 0px;
    text-align: center;
    font-size: 14px;
    font-weight: bold;
    padding: 5px;
  }
  .rate_value {
    text-align: center;
    margin: 0px;
    font-size: 12px;
    padding: 5px;
  }
}
.page-break { display:block; clear:both; page-break-after:always; }

.note-remarks-row {
  text-align: right;
  padding: 2px;
  margin: 2px;
  font-size: 12px;
  .note-remark-text {
    font-size: 10px;
    line-height: 1.5;
    font-weight: normal;
    width: 100%;
  }
}

.voyage-info {
  padding: 5px 0;
}

.quote-container {
  flex-flow: "row wrap";
  .all-row {
    flex: 1;
    flex-flow: "row wrap";
  }
}

.table-level-1 {
  background: #CECECE;
  p {
    font-size: 18px;
    font-weight: bold;
  }
}
.table-level-2 {
  p {
    font-size: 16px;
    font-weight: bold;
  }
}
.table-level-3 {
  p {
    font-size: 14px;
    font-weight: bold;
  }
}
.table-level-4 {
  p {
    font-size: 12px;
    font-weight: bold;
  }
}
.data-table {
  width: 100%;
  border-collapse: collapse;
  tr {
    width: 100%;
    p {
      margin: 5px;
    }
  }
  td {
     padding: 0;
     margin: 0;
  }
  .table-title {
    width: 40%;
    text-align: start;
    p {
      margin: 0;
      font-size: 12px;
    }
  }
  .table-quantity {
    width: 40%;
    padding-left: 20px;
    text-align: start;
    p {
      margin: 0;
      font-size: 12px;
    }
  }
  .table-value {
    width: 30%;
    text-align: end;
    span {
      margin: 0;
      font-size: 12px;
    }
  }
}
