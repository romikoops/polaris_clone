<%if user_profile.persisted? %>
  <div class="agent-info">
    <strong>Client</strong>
    <address>
      <p><strong><%= user_profile.full_name %></strong></p>
      <p><%= user_profile.email %></p>
      <p><%= user_profile.phone %></p>
      <p><%= user_profile.external_id %></p>
    </address>
  </div>
<% end %>
