<div class="container">
  <div class="row">
    <h2><%= @organization.slug %></h2>
  </div>
  <hr />
  <div>
    <%= render partial: 'filters' unless @stats.overview.empty? %>
    <%= render partial: 'metrics' unless @stats.overview.empty? %>
  </div>
  <div class="row">
    <div class="col">
      <div class="table-responsive">
        <h3>Per agent and date</h3>
        <table class="table table-striped">
          <thead>
            <tr>
              <th scope="col">Date</th>
              <th scope="col"># Individual Agents</th>
              <th scope="col"># <%= @stats.quotation_tool? ? 'Quotations' : 'Bookings' %></th>
              <th scope="col">&oslash; Time for Booking Process</th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: 'per_date_quotation_stats', collection: @stats.overview %>
          </tbody>
        </table>
      </div>
    </div>
    <div class="col">
      <div class="table-responsive">
        <h3>Per date</h3>
        <table class="table table-striped">
          <thead>
            <tr>
              <th scope="col">Date</th>
              <th scope="col">Email</th>
              <th scope="col">Company</th>
              <th scope="col"># <%= @stats.quotation_tool? ? 'Quotations' : 'Bookings' %></th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: 'per_date_and_agent_quotation_stats', collection: @stats.overview %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
